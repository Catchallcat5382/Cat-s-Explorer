ro",hrp);bg.MaxTorque=Vector3.new(100000,100000,100000);bg.P=9000;flyConn=RunService.RenderStepped:Connect(function(dt) if isTyping() then bv.Velocity=Vector3.zero;return;end local cam=workspace.CurrentCamera;local dir=Vector3.zero;if UserInputService:IsKeyDown(Enum.KeyCode.W) then dir+=cam.CFrame.LookVector end if UserInputService:IsKeyDown(Enum.KeyCode.S) then dir-=cam.CFrame.LookVector end if UserInputService:IsKeyDown(Enum.KeyCode.A) then dir-=cam.CFrame.RightVector end if UserInputService:IsKeyDown(Enum.KeyCode.D) then dir+=cam.CFrame.RightVector end if UserInputService:IsKeyDown(Enum.KeyCode.Space) then dir+=Vector3.yAxis end if UserInputService:IsKeyDown(Enum.KeyCode.LeftControl) then dir-=Vector3.yAxis end if (dir.Magnitude>0) then dir=dir.Unit * flySpeed ;end currentVel=currentVel:Lerp(dir,math.clamp(dt * accel ,0,1));bv.Velocity=currentVel;bg.CFrame=CFrame.new(hrp.Position,hrp.Position + cam.CFrame.LookVector );end);end LocalPlayer.CharacterAdded:Connect(stopFly);local FlyBtn=button(FlySec,"Fly",40,function() if flying then stopFly();FlyBtn.Text="Fly";else startFly();FlyBtn.Text="Unfly";end end);button(FlySec,"Speed -",80,function() flySpeed=math.clamp(flySpeed-10 ,10,300);updateFlyLabel();end);button(FlySec,"Speed +",120,function() flySpeed=math.clamp(flySpeed + 10 ,10,300);updateFlyLabel();end);local EspSec=section("ESP",620,130);local espEnabled=false;local esp={};local espCons={};local function clearESP() for _,h in pairs(esp) do if h then h:Destroy();end end for _,c in ipairs(espCons) do if c then c:Disconnect();end end table.clear(esp);table.clear(espCons);end local function applyESP(p) if ( not espEnabled or (p==LocalPlayer) or  not p.Character) then return;end if esp[p] then esp[p]:Destroy();end local h=Instance.new("Highlight",CoreGui);h.Adornee=p.Character;h.FillColor=C.PURPLE;h.FillTransparency=0.85;h.DepthMode=Enum.HighlightDepthMode.AlwaysOnTop;esp[p]=h;end local function hookESP(p) local c=p.CharacterAdded:Connect(function() task.wait(0.25);applyESP(p);end);table.insert(espCons,c);end for _,p in ipairs(Players:GetPlayers()) do hookESP(p);end Players.PlayerAdded:Connect(hookESP);Players.PlayerRemoving:Connect(function(p) if esp[p] then esp[p]:Destroy();esp[p]=nil;end end);button(EspSec,"Toggle ESP",40,function() espEnabled= not espEnabled;clearESP();if espEnabled then for _,p in ipairs(Players:GetPlayers()) do applyESP(p);end end end);local ObjSec=section("OBJECTIVES",770,140);local objectivesEnabled=false;local objItems={};local objCons={};local function clearObjectives() for _,o in ipairs(objItems) do for _,v in ipairs(o) do if v then v:Destroy();end end end for _,c in ipairs(objCons) do if c then c:Disconnect();end end table.clear(objItems);table.clear(objCons);end local function getObjectives() return workspace:FindFirstChild("Game") and workspace.Game:FindFirstChild("Map") and workspace.Game.Map:FindFirstChild("Parts") and workspace.Game.Map.Parts:FindFirstChild("Objectives") ;end local function addObjective(part) local h=Instance.new("Highlight",CoreGui);h.Adornee=part;h.FillColor=C.GREEN;h.FillTransparency=0.2;local bb=Instance.new("BillboardGui",CoreGui);bb.Adornee=part;bb.Size=UDim2.fromOffset(120,30);bb.AlwaysOnTop=true;local txt=Instance.new("TextLabel",bb);txt.Size=UDim2.fromScale(1,1);txt.BackgroundTransparency=1;txt.TextColor3=C.GREEN;txt.Font=Enum.Font.GothamBold;txt.TextSize=14;local c=RunService.RenderStepped:Connect(function() if ( not objectivesEnabled or  not part.Parent) then c:Disconnect();return;end local hrp=LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("HumanoidRootPart") ;if hrp then txt.Text=math.floor((hrp.Position-part.Position).Magnitude)   .. " studs" ;end end);table.insert(objItems,{h,bb});table.insert(objCons,c);end button(ObjSec,"Show Objectives",40,function() objectivesEnabled= not objectivesEnabled;clearObjectives();if objectivesEnabled then local f=getObjectives();if f then for _,v in ipairs(f:GetDescendants()) do if v:IsA("BasePart") then addObjective(v);end end table.insert(objCons,f.DescendantAdded:Connect(function(v) if (objectivesEnabled and v:IsA("BasePart")) then addObjective(v);end end));end end end);local GameSec=section("GAME",940,120);button(GameSec,"Rejoin",34,function() stopFly();task.wait();TeleportService:Teleport(game.PlaceId,LocalPlayer);end);button(GameSec,"Refresh Character",70,function() if LocalPlayer.Character then LocalPlayer.Character:BreakJoints();end end);UserInputService.InputBegan:Connect(function(i,gp) if (gp or SCRIPT_CLOSED or isTyping()) then return;end if (i.KeyCode==FLY_TOGGLE_KEY) then if flying then stopFly();FlyBtn.Text="Fly";else startFly();FlyBtn.Text="Unfly";end elseif (i.KeyCode==COLLAPSE_KEY) then toggleUI();end end);LocalPlayer.CharacterAdded:Connect(function() task.wait(0.3);if noclip then setNoclip(true);end end);CloseBtn.MouseButton1Click:Connect(function() SCRIPT_CLOSED=true;looping=false;orbiting=false;stopFly();setNoclip(false);clearESP();clearObjectives();GUI:Destroy();end); end
