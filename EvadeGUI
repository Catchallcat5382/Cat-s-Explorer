 NoclipBtn=button(MoveSec,"Noclip: OFF",40,function() setNoclip( not noclip);NoclipBtn.Text=(noclip and "Noclip: ON") or "Noclip: OFF" ;end);local PlayerSec=section("PLAYER",140,260);local SearchBox=Instance.new("TextBox",PlayerSec);SearchBox.Size=UDim2.new(1, -16,0,30);SearchBox.Position=UDim2.fromOffset(8,34);SearchBox.PlaceholderText="Search player...";SearchBox.ClearTextOnFocus=false;SearchBox.TextColor3=C.TEXT;SearchBox.BackgroundColor3=C.BUTTON;Instance.new("UICorner",SearchBox);local Scroll=Instance.new("ScrollingFrame",PlayerSec);Scroll.Position=UDim2.fromOffset(8,70);Scroll.Size=UDim2.new(1, -16,0,100);Scroll.CanvasSize=UDim2.new();Scroll.BackgroundTransparency=1;local Layout=Instance.new("UIListLayout",Scroll);local loopTarget,orbitTarget;local looping,orbiting=false,false;local orbitAngle=0;local function instantGoto(p) local char=LocalPlayer.Character;if  not char then return;end local hrp=char:FindFirstChild("HumanoidRootPart");local hum=char:FindFirstChildOfClass("Humanoid");local thrp=p.Character and p.Character:FindFirstChild("HumanoidRootPart") ;if  not (hrp and hum and thrp) then return;end hum.PlatformStand=false;hrp.AssemblyLinearVelocity=Vector3.zero;hrp.AssemblyAngularVelocity=Vector3.zero;RunService.Heartbeat:Wait();hrp.CFrame=thrp.CFrame * CFrame.new(0,0, -2) ;end RunService.Heartbeat:Connect(function(dt) if (SCRIPT_CLOSED or flying) then return;end if (looping and loopTarget) then instantGoto(loopTarget);end if (orbiting and orbitTarget and orbitTarget.Character) then local hrp=orbitTarget.Character:FindFirstChild("HumanoidRootPart");local my=LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("HumanoidRootPart") ;if (hrp and my) then orbitAngle+=(dt * 2) my.CFrame=CFrame.new(hrp.Position + Vector3.new(math.cos(orbitAngle) * 6 ,2,math.sin(orbitAngle) * 6 ) ,hrp.Position);end end end);button(PlayerSec,"Orbit Player",180,function() orbiting= not orbiting;end);button(PlayerSec,"Loop Goto",220,function() looping= not looping;end);loca